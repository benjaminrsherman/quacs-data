{
  "name": "HASS Core",
  "type": "requirements",
  "all": [{
      "type": "numeric_restriction",
      "name": "20 Credit Minimum (Other restrictions apply based on your major)",
      "min_number_credits": 20,
      "value": {
        "type": "regex",
        "matches": [{
          "key": "dept",
          "regex": "(ARTS|LANG|LITR|GSAS|HCDE|IHSS|PHIL|STSH|WRIT|COGS|ECON|HSAS|HCDE|PSYC|STSS)"
        }]
      }
